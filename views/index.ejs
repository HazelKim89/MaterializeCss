<!DOCTYPE html>
<html>

<head>
  <title>
    <%= title %>
  </title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <!-- Import Google Font -->
  <link href="https://fonts.googleapis.com/css?family=Permanent+Marker" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Charm" rel="stylesheet">
  <!--Import Google Icon Font-->
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <!--Import materialize.css-->
  <link type="text/css" rel="stylesheet" href="/stylesheets/materialize.min.css" media="screen,projection" />
  <!--Let browser know website is optimized for mobile-->
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
</head>

<body>

  <div class="wrap">
    <div class="container">
      <div class="header">
        <h1>Sketch World</h1>
      </div>
    </div>
    <div class="container">
      <div class="row">
        <!-- Medium & Small size menu -->
        <div class="col s12 show-on-medium">
          <ul class="tabs">
            <li class="tab col s3"><a href="#">Main</a></li>
            <li class="tab col s3"><a href="#">About</a></li>
            <li class="tab col s3"><a href="#">Daily Sketch</a></li>
            <li class="tab col s3"><a href="#">Gallery</a></li>
            <li class="tab col s3"><a href="#">Comment</a></li>
            <li class="tab col s3"><a href="#">Sources</a></li>
          </ul>
        </div>
        <div id="sidenav" class="col m3 l3 hide-on-med-and-down">
          <ul id="nav-movile" class="sidenav-fixed collection">
            <li class="collection-item">Main</li>
            <li class="collection-item">About</li>
            <li class="collection-item">Daily Sketch</li>
            <li class="collection-item">Gallery</li>
            <li class="collection-item">Comment</li>
            <li class="collection-item">Sources</li>
          </ul>
        </div>
        <div class="col l9 m12 s12">
          <div class="row">
            <div class="col m6 l6 xl4">
              <img src="/images/main2.jpg" class="responsive-img">
            </div>
            <div class="col xl4">
              <img src="/images/main7.jpg" class="responsive-img">
            </div>
            <div class="col m6 l6 xl4">
              <img src="/images/main3.jpg" class="responsive-img">
            </div>
          </div>
        </div>
        <div class="col s12" id="footer">
          <h5 class="col s3"><a href="#"><img src="/images/github.png" width="20px">HazelKim</a></h5>
          <h5 class="col s6">Contact : hazel22kim@gmail.com</h5>
          <h5 class="col s3">Made By <a href="https://materializecss.com/">Materialize</a></h5>
        </div
      </div>
    </div>
  </div>

 <script type="text/javascript" src="/javascripts/materialize.min.js"></script>
 <script type="text/javascript">
  document.addEventListener('DOMContentLoaded', function(){
    var elems = document.querySelectorAll('.sidenav')
    var instances = M.Sidenav.init(elems)
    M.AutoInit();
  })
 </script>
</body>
<link rel='stylesheet' href="/stylesheets/index.css" />
</html>